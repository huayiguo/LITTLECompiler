PROGRAM fibonacci
BEGIN

	STRING dummy := "abcde";
	STRING space := " ";
	STRING eol := "\n";
	INT i,result; 


	FUNCTION INT F (INT n)
	BEGIN
		--WRITE(dummy,space,n);

		IF (n > 2)
			RETURN F(n-1)+F(n-2);
		ELSIF (n = 0)   --This is a comment
			RETURN 0;
          ELSIF (n = 1)
			RETURN 1;
          ELSIF (n = 2)
			RETURN 1;
		ENDIF
	END
	
	
	FUNCTION VOID main ()
	BEGIN
		
    INT i, end, result;
		READ(end);

	i := 0;
 	DO
		result := F(i);
		WRITE (i);
		WRITE (space);
		WRITE (result);
		WRITE (eol);
		i := i + 1;	
	WHILE (i != end);


	END

END	
